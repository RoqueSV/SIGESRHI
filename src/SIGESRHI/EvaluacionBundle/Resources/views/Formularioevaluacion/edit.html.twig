{% extends '::PrincipalBase.html.twig' %}

{% block principal -%}

    <h3 class="text-center">Modificar Formulario de evaluaci&oacute;n</h3>

    <form action="{{ path('formularioevaluacion_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
        <input type="hidden" name="_method" value="PUT" />
        {{ form_widget(edit_form) }}
        <p>
            <button type="submit">Edit</button>
        </p>
    </form>

        <ul class="record_actions">
    <li>
        <a href="{{ path('formularioevaluacion') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ path('formularioevaluacion_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
