	<queryString>
		<![CDATA[select concat(case  to_char(current_date,'DD')
when  '1' then 'uno'
when  '2' then 'dos'
when  '3' then 'tres'
when  '4' then 'cuatro'
when  '5' then 'cinco'
when  '6' then 'seis'
when  '7' then 'siete'
when  '8' then 'ocho'
when  '9' then 'nueve'
when  '10' then 'diez'
when  '11' then 'once'
when  '12' then 'doce'
when  '13' then 'trece'
when  '14' then 'catorce'
when  '15' then 'quince'
when  '16' then 'dieciséis'
when  '17' then 'diecisiete'
when  '18' then 'dieciocho'
when  '19' then 'diecinueve'
when  '20' then 'veinte'
when  '21' then 'veintiuno'
when  '22' then 'veintidos'
when  '23' then 'veintitres'
when  '24' then 'veinticuatro'
when  '25' then 'veinticinco'
when  '26' then 'veintiseis'
when  '27' then 'veintisiete'
when  '28' then 'veintiocho'
when  '29' then 'veintinueve'
when  '30' then 'treinta'
else 'treinta y uno' end,' dias del mes de ',to_char(current_date,'TMMonth'),' de dos mil ', case  to_char(current_date,'YY') when '14' then 'Catorce'
when  '15' then 'Quince'
when  '16' then 'Dieciséis'
when  '17' then 'Diecisiete'
when  '18' then 'Dieciocho'
when  '19' then 'Diecinueve'
when  '20' then 'Veinte'
when  '21' then 'Veintiuno'
when  '22' then 'Veintidos'
when  '23' then 'Veintitres'
when  '24' then 'Veinticuatro'
when  '25' then 'Veinticinco'
when  '26' then 'Veintiseis'
when  '27' then 'Veintisiete'
when  '28' then 'Veintiocho'
when  '29' then 'Veintinueve'
else 'Treinta' end ) as fechaenletras,e.id,fotografia,nombreempleado,concat(substr(hs.dui,1,8),'-',substr(hs.dui,9))as dui,hs.lugardui,hs.lugarnac,to_char(current_date,'Day "días del mes de" TMMonth "del dos mil" YEAR' ) as fechaenletras, to_char(hs.fechanac,'DD "de" TMMonth "de" YYYY') as fechanac,hs.direccion,case when hs.estadoCivil= 'S' then 'Soltero(a)'
	   when hs.estadoCivil= 'A' then 'Acompañado(a)'
	   when hs.estadoCivil= 'V' then 'Viudo(a)'
	   when hs.estadoCivil= 'D' then 'Divorciado(a)'
	   else 'Casado(a)' end as estadocivil,hs.telefonofijo as telefonos,coalesce(hs.isss,'--') as isss,concat(substr(hs.nit,1,4),'-',substr(hs.nit,5,6),'-',substr(hs.nit,11,3),'-',substr(hs.nit,14)) as nit,to_char(fechaingreso,'DD "de" TMMonth "de" YYYY') as fechaingreso,to_char(hs.sueldoinicial,'$9,999.00') as sueldoinicial,coalesce(hs.informacionadicional,' ') as informacionadicional,hs.cargo,destacadoen,educacion,codigoempleado from solicitudempleo se join expediente e on(se.idexpediente=e.id)
join empleado emp on (e.id=emp.idexpediente) join hojaservicio  hs on (e.id=hs.idexpediente) where e.id=$P{idexp}]]>
	</queryString>
