	<queryString>
		<![CDATA[select fotografia,nombres||' '||primerapellido||' '||segundoapellido as nombrecompleto,dui,lugardui,concat(lugarnac,' ',fechanac) as datosnac, concat(calle,' ',colonia ) as direccion,case when estadoCivil= 'S' then 'Soltero(a)'
	   when estadoCivil= 'A' then 'Acompañado(a)'
	   when estadoCivil= 'V' then 'Viudo(a)'
	   when estadoCivil= 'D' then 'Divorciado(a)'
	   else 'Casado(a)' end as estadocivil,concat(telefonofijo,' ',telefonomovil) as telefonos,isss,nit,fechainiciocontratacion,sueldoinicial,informacionadicional,nombreplaza,nombreunidad,nombretitulo from solicitudempleo se join expediente e on(se.idexpediente=e.id) join empleado emp on (e.id=emp.idexpediente) join contratacion c on (c.idempleado=emp.id) join unidadorganizativa u on(c.idunidad=u.id) join plaza p on(c.idplaza=p.id) join tituloplaza tp on(p.id=tp.idplaza) join titulo t on(tp.idtitulo=t.id) where e.id=$P{idexp}]]>
