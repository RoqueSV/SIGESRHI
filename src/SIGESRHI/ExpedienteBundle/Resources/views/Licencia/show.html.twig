{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Licencia</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Concepto</th>
                <td>{{ entity.concepto }}</td>
            </tr>
            <tr>
                <th>Duraciondias</th>
                <td>{{ entity.duraciondias }}</td>
            </tr>
            <tr>
                <th>Duracionhoras</th>
                <td>{{ entity.duracionhoras }}</td>
            </tr>
            <tr>
                <th>Duracionminutos</th>
                <td>{{ entity.duracionminutos }}</td>
            </tr>
            <tr>
                <th>Congoce</th>
                <td>{{ entity.congoce }}</td>
            </tr>
            <tr>
                <th>Fechainiciolic</th>
                <td>{{ entity.fechainiciolic|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechafinlic</th>
                <td>{{ entity.fechafinlic|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Horainiciolic</th>
                <td>{{ entity.horainiciolic }}</td>
            </tr>
            <tr>
                <th>Horafinlic</th>
                <td>{{ entity.horafinlic }}</td>
            </tr>
            <tr>
                <th>Fechapermiso</th>
                <td>{{ entity.fechapermiso|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('licencia') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('licencia_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('licencia_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
