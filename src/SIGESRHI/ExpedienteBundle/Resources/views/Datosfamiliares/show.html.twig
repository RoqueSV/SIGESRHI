{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Datosfamiliares</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombrefamiliar</th>
                <td>{{ entity.nombrefamiliar }}</td>
            </tr>
            <tr>
                <th>Direccionfamiliar</th>
                <td>{{ entity.direccionfamiliar }}</td>
            </tr>
            <tr>
                <th>Telefonofamiliar</th>
                <td>{{ entity.telefonofamiliar }}</td>
            </tr>
            <tr>
                <th>Parentesco</th>
                <td>{{ entity.parentesco }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('datosfamiliares') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('datosfamiliares_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('datosfamiliares_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
