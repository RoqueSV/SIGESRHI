{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Datosempleo</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombreempresa</th>
                <td>{{ entity.nombreempresa }}</td>
            </tr>
            <tr>
                <th>Direccionempresa</th>
                <td>{{ entity.direccionempresa }}</td>
            </tr>
            <tr>
                <th>Telefonoempresa</th>
                <td>{{ entity.telefonoempresa }}</td>
            </tr>
            <tr>
                <th>Fechainiciolaboral</th>
                <td>{{ entity.fechainiciolaboral|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechafinlaboral</th>
                <td>{{ entity.fechafinlaboral|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Jefeinmediato</th>
                <td>{{ entity.jefeinmediato }}</td>
            </tr>
            <tr>
                <th>Cargodesempenado</th>
                <td>{{ entity.cargodesempenado }}</td>
            </tr>
            <tr>
                <th>Sueldo</th>
                <td>{{ entity.sueldo }}</td>
            </tr>
            <tr>
                <th>Motivoretiro</th>
                <td>{{ entity.motivoretiro }}</td>
            </tr>
            <tr>
                <th>Tipodatoempleo</th>
                <td>{{ entity.tipodatoempleo }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('datosempleo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('datosempleo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('datosempleo_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
