{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Solicitudempleo list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Numsolicitud</th>
                <th>Apellidocasada</th>
                <th>Primerapellido</th>
                <th>Segundoapellido</th>
                <th>Nombres</th>
                <th>Colonia</th>
                <th>Calle</th>
                <th>Estadocivil</th>
                <th>Telefonofijo</th>
                <th>Telefonomovil</th>
                <th>Email</th>
                <th>Lugarnac</th>
                <th>Fehanac</th>
                <th>Dui</th>
                <th>Lugardui</th>
                <th>Fechadui</th>
                <th>Nit</th>
                <th>Isss</th>
                <th>Nup</th>
                <th>Nip</th>
                <th>Sexo</th>
                <th>Fotografia</th>
                <th>Fecharegistro</th>
                <th>Fechamodificacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('solicitud_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.numsolicitud }}</td>
                <td>{{ entity.apellidocasada }}</td>
                <td>{{ entity.primerapellido }}</td>
                <td>{{ entity.segundoapellido }}</td>
                <td>{{ entity.nombres }}</td>
                <td>{{ entity.colonia }}</td>
                <td>{{ entity.calle }}</td>
                <td>{{ entity.estadocivil }}</td>
                <td>{{ entity.telefonofijo }}</td>
                <td>{{ entity.telefonomovil }}</td>
                <td>{{ entity.email }}</td>
                <td>{{ entity.lugarnac }}</td>
                <td>{% if entity.fehanac %}{{ entity.fehanac|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.dui }}</td>
                <td>{{ entity.lugardui }}</td>
                <td>{% if entity.fechadui %}{{ entity.fechadui|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.nit }}</td>
                <td>{{ entity.isss }}</td>
                <td>{{ entity.nup }}</td>
                <td>{{ entity.nip }}</td>
                <td>{{ entity.sexo }}</td>
                <td>{{ entity.fotografia }}</td>
                <td>{% if entity.fecharegistro %}{{ entity.fecharegistro|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechamodificacion %}{{ entity.fechamodificacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('solicitud_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('solicitud_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('solicitud_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
