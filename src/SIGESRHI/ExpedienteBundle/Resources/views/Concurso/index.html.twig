{% extends '::PrincipalBase.html.twig' %}

{% block principal -%}
    <h1>Concurso list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Codigoconcurso</th>
                <th>Fechaapertura</th>
                <th>Fechacierre</th>
                <th>Numeroacta</th>
                <th>Anoacta</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('concurso_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.codigoconcurso }}</td>
                <td>{% if entity.fechaapertura %}{{ entity.fechaapertura|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechacierre %}{{ entity.fechacierre|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.numeroacta }}</td>
                <td>{{ entity.anoacta }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('concurso_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('concurso_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('concurso_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
