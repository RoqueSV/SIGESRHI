{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Centrounidad</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombrecentro</th>
                <td>{{ entity.nombrecentro }}</td>
            </tr>
            <tr>
                <th>Especialidad</th>
                <td>{{ entity.especialidad }}</td>
            </tr>
            <tr>
                <th>Direccioncentro</th>
                <td>{{ entity.direccioncentro }}</td>
            </tr>
            <tr>
                <th>Telefonocentro</th>
                <td>{{ entity.telefonocentro }}</td>
            </tr>
            <tr>
                <th>Extensioncentro</th>
                <td>{{ entity.extensioncentro }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('centrounidad') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('centrounidad_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('centrounidad_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
