{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Centrounidad list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombrecentro</th>
                <th>Especialidad</th>
                <th>Direccioncentro</th>
                <th>Telefonocentro</th>
                <th>Extensioncentro</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('centrounidad_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.nombrecentro }}</td>
                <td>{{ entity.especialidad }}</td>
                <td>{{ entity.direccioncentro }}</td>
                <td>{{ entity.telefonocentro }}</td>
                <td>{{ entity.extensioncentro }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('centrounidad_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('centrounidad_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('centrounidad_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
