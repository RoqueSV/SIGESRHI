{% extends '::PrincipalBase.html.twig' %}

{% block principal -%}
    <h1>Acceso edit</h1>
     <br />
    <fieldset>
        <legend> Actualizar opciones de acceso </legend>
    <form action="{{ path('acceso_update', { 'id': entity.id }) }}" class="_form" method="post" {{ form_enctype(edit_form) }}>
        <input type="hidden" name="_method" value="PUT" />
        {{ form_widget(edit_form) }}
        <p>
            <button type="submit">Edit</button>
        </p>
    </form>
    </fieldset>
        <ul class="record_actions">
    <li>
        <a href="{{ path('acceso') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ path('acceso_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
